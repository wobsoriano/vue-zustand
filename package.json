{
  "name": "vue-zustand",
  "version": "0.6.0",
  "description": "ðŸ» Bear necessities for Vue.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-zustand.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format esm,cjs --external zustand/vanilla",
    "build": "pnpm run build-fast --dts-resolve",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint --ext=js,ts .",
    "lint:fix": "eslint --ext=js,ts . --fix"
  },
  "peerDependencies": {
    "vue": ">=3.2.0",
    "zustand": ">=4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.3",
    "@vue/test-utils": "^2.3.2",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "jsdom": "^22.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1",
    "vue": "^3.3.4",
    "zustand": "^4.3.8"
  },
  "homepage": "https://github.com/wobsoriano/vue-zustand#readme",
  "eslintConfig": {
    "extends": "@antfu"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2"
  }
}
